{
  "name": "result-type-ts",
  "version": "0.1.0",
  "description": "A TypeScript library for the Result type",
  "keywords": [
    "Result",
    "Monad",
    "functional",
    "Either",
    "Algebraic-Data-Types"
  ],
  "homepage": "https://github.com/ootidea/result-type-ts#readme",
  "bugs": {
    "url": "https://github.com/ootidea/result-type-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ootidea/result-type-ts.git"
  },
  "license": "CC0-1.0",
  "author": "ootidea",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "format": "run-p format:*",
    "format:prettier": "prettier --write \"src/**/*.ts\"",
    "format:package-json": "sort-package-json",
    "postversion": "git push && git push --tags && npm publish ./",
    "prepublishOnly": "npm run build",
    "test": "vitest --silent=false",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "sort-package-json": "^2.5.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "volta": {
    "node": "18.12.1"
  }
}
